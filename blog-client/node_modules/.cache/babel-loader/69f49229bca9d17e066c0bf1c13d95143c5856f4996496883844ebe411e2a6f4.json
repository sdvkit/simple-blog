{"ast":null,"code":"import Menubar from 'primevue/menubar';\nimport Button from \"primevue/button\";\nimport Toast from 'primevue/toast';\nimport Dialog from 'primevue/dialog';\nimport InputText from \"primevue/inputtext\";\nimport Textarea from \"primevue/textarea\";\nimport InlineMessage from 'primevue/inlinemessage';\nimport OverlayPanel from 'primevue/overlaypanel';\nimport DataTable from \"primevue/datatable\";\nimport Column from \"primevue/column\";\nimport posts from '../posts.json';\nimport Avatar from \"primevue/avatar\";\nimport ConfirmDialog from 'primevue/confirmdialog';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      searchValue: '',\n      displayModal: false,\n      postTitle: '',\n      postDescription: '',\n      showTitleError: false,\n      showDescriptionError: false,\n      items: [{\n        label: 'Home',\n        icon: 'bx bx-home-alt',\n        to: '/'\n      }, {\n        label: 'Profile',\n        icon: 'bx bx-user',\n        to: '/profile'\n      }, {\n        label: 'Login',\n        to: '/login'\n      }, {\n        label: 'Registration',\n        to: '/registration'\n      }],\n      posts\n    };\n  },\n  methods: {\n    getProfile(name) {\n      document.location.href = `/${name}`;\n    },\n    openModal() {\n      this.displayModal = true;\n    },\n    toggleSearch(event) {\n      this.$refs.op.toggle(event);\n    },\n    savePost() {\n      if (this.postTitle.length < 5) {\n        this.showTitleError = true;\n        return;\n      }\n      this.showTitleError = false;\n      if (this.postDescription.length < 5) {\n        this.showDescriptionError = true;\n        return;\n      }\n      this.showDescriptionError = false;\n      this.displayModal = false;\n      this.postTitle = '';\n      this.postDescription = '';\n      this.$toast.add({\n        severity: 'success',\n        summary: 'Successfully saved',\n        detail: 'New post has been created',\n        life: 6000\n      });\n    }\n  },\n  components: {\n    Menubar,\n    Button,\n    Toast,\n    Dialog,\n    InputText,\n    Textarea,\n    InlineMessage,\n    OverlayPanel,\n    DataTable,\n    Column,\n    Avatar,\n    ConfirmDialog\n  }\n};","map":{"version":3,"mappings":"AAuEA,OAAOA,OAAM,MAAO,kBAAkB;AACtC,OAAOC,MAAK,MAAO,iBAAiB;AACpC,OAAOC,KAAI,MAAO,gBAAgB;AAClC,OAAOC,MAAK,MAAO,iBAAiB;AACpC,OAAOC,SAAQ,MAAO,oBAAoB;AAC1C,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,aAAY,MAAO,wBAAwB;AAClD,OAAOC,YAAW,MAAO,uBAAuB;AAChD,OAAOC,SAAQ,MAAO,oBAAoB;AAC1C,OAAOC,MAAK,MAAO,iBAAiB;AACpC,OAAOC,KAAI,MAAO,eAAe;AACjC,OAAOC,MAAK,MAAO,iBAAiB;AACpC,OAAOC,aAAY,MAAO,wBAAwB;AAElD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,KAAK;MACnBC,SAAS,EAAE,EAAE;MACbC,eAAe,EAAE,EAAE;MACnBC,cAAc,EAAE,KAAK;MACrBC,oBAAoB,EAAE,KAAK;MAC3BC,KAAK,EAAE,CACL;QAAEC,KAAK,EAAE,MAAM;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,EAAE,EAAE;MAAI,CAAC,EAClD;QAAEF,KAAK,EAAE,SAAS;QAAEC,IAAI,EAAE,YAAY;QAAEC,EAAE,EAAE;MAAW,CAAC,EACxD;QAAEF,KAAK,EAAE,OAAO;QAAEE,EAAE,EAAE;MAAS,CAAC,EAChC;QAAEF,KAAK,EAAE,cAAc;QAAEE,EAAE,EAAE;MAAgB,EAC9C;MACDd;IACF;EACF,CAAC;EACDe,OAAO,EAAE;IACPC,UAAU,CAACb,IAAI,EAAE;MACfc,QAAQ,CAACC,QAAQ,CAACC,IAAI,GAAG,IAAGhB,IAAK,EAAC;IACpC,CAAC;IACDiB,SAAS,GAAG;MACV,IAAI,CAACd,YAAW,GAAI,IAAI;IAC1B,CAAC;IACDe,YAAY,CAACC,KAAK,EAAE;MAClB,IAAI,CAACC,KAAK,CAACC,EAAE,CAACC,MAAM,CAACH,KAAK,CAAC;IAC7B,CAAC;IACDI,QAAQ,GAAG;MACT,IAAI,IAAI,CAACnB,SAAS,CAACoB,MAAK,GAAI,CAAC,EAAE;QAC7B,IAAI,CAAClB,cAAa,GAAI,IAAI;QAC1B;MACF;MACA,IAAI,CAACA,cAAa,GAAI,KAAK;MAE3B,IAAI,IAAI,CAACD,eAAe,CAACmB,MAAK,GAAI,CAAC,EAAE;QACnC,IAAI,CAACjB,oBAAmB,GAAI,IAAI;QAChC;MACF;MACA,IAAI,CAACA,oBAAmB,GAAI,KAAK;MAEjC,IAAI,CAACJ,YAAW,GAAI,KAAK;MACzB,IAAI,CAACC,SAAQ,GAAI,EAAE;MACnB,IAAI,CAACC,eAAc,GAAI,EAAE;MACzB,IAAI,CAACoB,MAAM,CAACC,GAAG,CAAC;QAACC,QAAQ,EAAC,SAAS;QAAEC,OAAO,EAAE,oBAAoB;QAAEC,MAAM,EAAC,2BAA2B;QAAEC,IAAI,EAAE;MAAI,CAAC,CAAC;IACtH;EACF,CAAC;EACDC,UAAU,EAAE;IACV5C,OAAO;IACPC,MAAM;IACNC,KAAK;IACLC,MAAM;IACNC,SAAS;IACTC,QAAQ;IACRC,aAAa;IACbC,YAAY;IACZC,SAAS;IACTC,MAAM;IACNE,MAAM;IACNC;EACF;AACF","names":["Menubar","Button","Toast","Dialog","InputText","Textarea","InlineMessage","OverlayPanel","DataTable","Column","posts","Avatar","ConfirmDialog","name","data","searchValue","displayModal","postTitle","postDescription","showTitleError","showDescriptionError","items","label","icon","to","methods","getProfile","document","location","href","openModal","toggleSearch","event","$refs","op","toggle","savePost","length","$toast","add","severity","summary","detail","life","components"],"sourceRoot":"","sources":["/Users/nikitasudaev/WebstormProjects/blog-client/src/App.vue"],"sourcesContent":["<template>\n  <Toast />\n  <ConfirmDialog></ConfirmDialog>\n\n  <Dialog header=\"Create new post\" v-model:visible=\"displayModal\" :breakpoints=\"{'960px': '75vw', '640px': '90vw'}\" :style=\"{ width: '40vw' }\">\n    <h3>Post title</h3>\n    <InputText id=\"post-title\" type=\"text\" v-model=\"postTitle\" :style=\"{ width: '100%' }\" placeholder=\"Your Title\" />\n    <p>\n      <InlineMessage v-if=\"showTitleError\">Length of Title must be more than 5</InlineMessage>\n    </p>\n\n    <h3>Post description</h3>\n    <Textarea v-model=\"postDescription\" :autoResize=\"true\" rows=\"5\" :style=\"{ width: '100%' }\" placeholder=\"Your Description\" />\n    <p>\n      <InlineMessage v-if=\"showDescriptionError\">Length of Description must be more than 5</InlineMessage>\n    </p>\n\n    <template #footer>\n      <Button label=\"Save post\" icon=\"pi pi-check\" @click=\"savePost\" autofocus />\n    </template>\n  </Dialog>\n\n  <Menubar :model=\"items\" :style=\"{\n    position: 'fixed',\n    width: '100%',\n    zIndex: '100'\n  }\">\n    <template #start>\n      <img alt=\"logo\" src=\"./assets/logo.png\" :style=\"{ height: '40px' }\" class=\"mr-2\">\n    </template>\n    <template #end>\n      <span class=\"p-input-icon-left\">\n        <i class=\"pi pi-search\" />\n        <InputText type=\"text\" v-model=\"searchValue\" placeholder=\"Search author\" :style=\"{\n          width: '180px',\n          marginRight: '5px'\n        }\" @keydown=\"toggleSearch\" />\n        <OverlayPanel ref=\"op\" appendTo=\"body\" id=\"overlay_panel\" style=\"width: 450px\" :breakpoints=\"{'960px': '75vw'}\">\n        <DataTable :value=\"posts\" :scrollable=\"true\" scrollHeight=\"350px\" :lazy=\"true\" :row-style=\"{ cursor: 'pointer' }\">\n          <Column header=\"Results:\">\n            <template #body=\"slotProps\">\n              <div @click=\"getProfile(slotProps.data.author)\">\n                <Avatar class=\"mr-2\" size=\"large\" :style=\"{\n                  background: 'url(\\'' + slotProps.data.icon + '\\') center no-repeat',\n                  color: '#ffffff',\n                  height: '50px',\n                  width: '50px',\n                  backgroundSize: '70px',\n                  border: '3px solid #ffffff',\n                  float: 'left'\n                }\" shape=\"circle\"/>\n                <span :style=\"{ marginLeft: '20px', marginTop: '30px' }\">\n                  {{ slotProps.data.author }}\n                </span>\n                <span :style=\"{ marginLeft: '30px', marginTop: '30px' }\">\n                  {{ slotProps.data.author }}123@gmail.com\n                </span>\n              </div>\n            </template>\n          </Column>\n        </DataTable>\n      </OverlayPanel>\n      </span>\n      <Button label=\"\" icon=\"bx bx-plus\" iconPos=\"left\" @click=\"openModal\" />\n    </template>\n  </Menubar>\n\n  <router-view />\n</template>\n\n<script>\nimport Menubar from 'primevue/menubar';\nimport Button from \"primevue/button\";\nimport Toast from 'primevue/toast';\nimport Dialog from 'primevue/dialog';\nimport InputText from \"primevue/inputtext\";\nimport Textarea from \"primevue/textarea\";\nimport InlineMessage from 'primevue/inlinemessage';\nimport OverlayPanel from 'primevue/overlaypanel';\nimport DataTable from \"primevue/datatable\";\nimport Column from \"primevue/column\";\nimport posts from '../posts.json';\nimport Avatar from \"primevue/avatar\";\nimport ConfirmDialog from 'primevue/confirmdialog';\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      searchValue: '',\n      displayModal: false,\n      postTitle: '',\n      postDescription: '',\n      showTitleError: false,\n      showDescriptionError: false,\n      items: [\n        { label: 'Home', icon: 'bx bx-home-alt', to: '/' },\n        { label: 'Profile', icon: 'bx bx-user', to: '/profile' },\n        { label: 'Login', to: '/login' },\n        { label: 'Registration', to: '/registration' }\n      ],\n      posts\n    }\n  },\n  methods: {\n    getProfile(name) {\n      document.location.href= `/${name}`;\n    },\n    openModal() {\n      this.displayModal = true;\n    },\n    toggleSearch(event) {\n      this.$refs.op.toggle(event);\n    },\n    savePost() {\n      if (this.postTitle.length < 5) {\n        this.showTitleError = true;\n        return;\n      }\n      this.showTitleError = false;\n\n      if (this.postDescription.length < 5) {\n        this.showDescriptionError = true;\n        return;\n      }\n      this.showDescriptionError = false;\n\n      this.displayModal = false;\n      this.postTitle = '';\n      this.postDescription = '';\n      this.$toast.add({severity:'success', summary: 'Successfully saved', detail:'New post has been created', life: 6000});\n    }\n  },\n  components: {\n    Menubar,\n    Button,\n    Toast,\n    Dialog,\n    InputText,\n    Textarea,\n    InlineMessage,\n    OverlayPanel,\n    DataTable,\n    Column,\n    Avatar,\n    ConfirmDialog\n  }\n}\n</script>\n\n<style>\n  body {\n    margin: 0 auto;\n    padding: 0;\n  }\n\n  #app {\n    font-family: Avenir, Helvetica, Arial, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    color: #2c3e50;\n  }\n\n  .p-menubar-start {\n    margin-left: 25px;\n    margin-right: 20px;\n  }\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}