{"ast":null,"code":"import Menubar from 'primevue/menubar';\nimport Button from \"primevue/button\";\nimport Toast from 'primevue/toast';\nimport Dialog from 'primevue/dialog';\nimport InputText from \"primevue/inputtext\";\nimport Textarea from \"primevue/textarea\";\nimport InlineMessage from 'primevue/inlinemessage';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      searchValue: '',\n      displayModal: false,\n      postTitle: '',\n      postDescription: '',\n      showErrorStatus: false,\n      items: [{\n        label: 'Home',\n        icon: 'bx bx-home-alt',\n        to: '/'\n      }, {\n        label: 'Profile',\n        icon: 'bx bx-user',\n        to: '/profile'\n      }]\n    };\n  },\n  methods: {\n    openModal() {\n      this.displayModal = true;\n    },\n    savePost() {\n      if (this.postTitle.length < 5 || this.postDescription.length < 5) {\n        this.displayModal = false;\n        this.postTitle = '';\n        this.postDescription = '';\n        this.$toast.add({\n          severity: 'success',\n          summary: 'Successfully saved',\n          detail: 'New post has been created',\n          life: 6000\n        });\n      } else {\n        this.showErrorStatus = true;\n      }\n    }\n  },\n  components: {\n    Menubar,\n    Button,\n    Toast,\n    Dialog,\n    InputText,\n    Textarea,\n    InlineMessage\n  }\n};","map":{"version":3,"mappings":"AA6CA,OAAOA,OAAM,MAAO,kBAAkB;AACtC,OAAOC,MAAK,MAAO,iBAAiB;AACpC,OAAOC,KAAI,MAAO,gBAAgB;AAClC,OAAOC,MAAK,MAAO,iBAAiB;AACpC,OAAOC,SAAQ,MAAO,oBAAoB;AAC1C,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,aAAY,MAAO,wBAAwB;AAElD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,KAAK;MACnBC,SAAS,EAAE,EAAE;MACbC,eAAe,EAAE,EAAE;MACnBC,eAAe,EAAE,KAAK;MACtBC,KAAK,EAAE,CACL;QAAEC,KAAK,EAAE,MAAM;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,EAAE,EAAE;MAAI,CAAC,EAClD;QAAEF,KAAK,EAAE,SAAS;QAAEC,IAAI,EAAE,YAAY;QAAEC,EAAE,EAAE;MAAW,CAAC;IAE5D;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,IA