s=\"p-input-icon-left\">\n        <i class=\"pi pi-search\" />\n        <InputText type=\"text\" v-model=\"searchValue\" placeholder=\"Search\" :style=\"{\n          width: '180px',\n          marginRight: '5px'\n        }\" />\n      </span>\n      <Button label=\"\" icon=\"bx bx-plus\" iconPos=\"left\" @click=\"openModal\" />\n    </template>\n  </Menubar>\n\n  <router-view />\n</template>\n\n<script>\nimport Menubar from 'primevue/menubar';\nimport Button from \"primevue/button\";\nimport Toast from 'primevue/toast';\nimport Dialog from 'primevue/dialog';\nimport InputText from \"primevue/inputtext\";\nimport Textarea from \"primevue/textarea\";\nimport InlineMessage from 'primevue/inlinemessage';\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      searchValue: '',\n      displayModal: false,\n      postTitle: '',\n      postDescription: '',\n      showErrorStatus: false,\n      items: [\n        { label: 'Home', icon: 'bx bx-home-alt', to: '/' },\n        { label: 'Profile', icon: 'bx bx-user', to: '/profile' },\n      ]\n    }\n  },\n  methods: {\n    openModal() {\n      this.displayModal = true;\n    },\n    savePost() {\n      if (this.postTitle.length < 5 || this.postDescription.length < 5) {\n        this.displayModal = false;\n        this.postTitle = '';\n        this.postDescription = '';\n        this.$toast.add({severity:'success', summary: 'Successfully saved', detail:'New post has been created', life: 6000});\n      } else {\n        this.showErrorStatus = true;\n      }\n    }\n  },\n  components: {\n    Menubar,\n    Button,\n    Toast,\n    Dialog,\n    InputText,\n    Textarea,\n    InlineMessage\n  }\n}\n</script>\n\n<style>\n  body {\n    margin: 0 auto;\n    padding: 0;\n  }\n\n  #app {\n    font-family: Avenir, Helvetica, Arial, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    color: #2c3e50;\n  }\n\n  .p-menubar-start {\n    margin-left: 25px;\n    margin-right: 20px;\n  }\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}